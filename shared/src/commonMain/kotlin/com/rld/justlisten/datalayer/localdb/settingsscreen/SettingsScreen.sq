CREATE TABLE SettingsInfo (
     id INTEGER PRIMARY KEY DEFAULT 1,
     hasNavigationFundOn INTEGER AS Boolean NOT NULL,
     isDarkThemeOn  INTEGER  AS Boolean  NOT NULL
);

INSERT INTO SettingsInfo(hasNavigationFundOn, isDarkThemeOn) VALUES (1, 1);

getSettingsInfo:
SELECT * FROM SettingsInfo;

upsertSettingsInfo {
    REPLACE INTO SettingsInfo(id, hasNavigationFundOn, isDarkThemeOn)
    VALUES(1, :hasNavigationFundOn, :isDarkThemeOn) ;
}